{
  "name": "StarterKit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-react-native",
    "moduleNameMapper": {
      "\\./datepicker$": "<rootDir>/node_modules/tcomb-form-native/lib/templates/bootstrap/datepicker.ios.js",
      "\\./select$": "<rootDir>/node_modules/tcomb-form-native/lib/templates/bootstrap/select.ios.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|tcomb-form-native|apsl-react-native-button)"
    ],
    "collectCoverage": true,
    "verbose": true
  },
  "dependencies": {
    "react": "15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-native": "^0.37.0",
    "react-native-google-analytics-bridge": "^4.0.1",
    "react-native-navbar": "^1.5.4",
    "react-native-side-menu": "^0.20.1",
    "react-native-tab-view": "0.0.40",
    "react-native-vector-icons": "^3.0.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "tcomb-form-native": "^0.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-jest": "^17.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-register": "^6.16.3",
    "jest": "^17.0.3",
    "jest-react-native": "^17.0.3",
    "react-native-looped-carousel": "0.1.4",
    "react-native-navbar": "1.5.4",
    "react-test-renderer": "^15.3.2"
  }
}
